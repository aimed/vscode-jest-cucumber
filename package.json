{
	"name": "completions-sample",
	"displayName": "Completion Item Provider Sample",
	"version": "0.0.2",
	"publisher": "vscode-samples",
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-extension-samples"
	},
	"bugs": {
		"url": "https://github.com/Microsoft/vscode-extension-samples/issues"
	},
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"contributes": {
		"commands": [
			{
				"command": "extension.jest-cucumber.defineFeatures",
				"title": "Define Features"
			}
		]
	},
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "tslint -p ./",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^10.17.18",
		"@types/vscode": "^1.43.0",
		"tslint": "^5.20.1",
		"typescript": "^3.8.3"
	},
	"dependencies": {
		"@cucumber/messages": "^11.1.1",
		"gherkin": "^9.0.0"
	}
}
